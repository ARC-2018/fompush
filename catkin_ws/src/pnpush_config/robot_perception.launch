<?xml version="1.0" ?>
<launch>
  <arg name="gui" default="true" />
  <arg name="have_robot" default="true" />
  <arg name="do_fitting" default="false" />

  
  <arg name="irb_120" default="true" />
  
  <param name="use_gui" value="$(arg gui)"/>
  <param name="robot_description" textfile="$(find pnpush_config)/models/IRB120/irb_120.urdf" />
    
  <node if="$(arg irb_120)" pkg="tf" type="static_transform_publisher" name="baselink_broadcaster" args="0 0 0 0 0 0 map base_link 50" />

  <node unless="$(arg have_robot)" name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" >
    <rosparam unless="$(arg have_robot)" command="load" file="$(find pnpush_config)/source_list_param.yaml" />
  </node>
  
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher" respawn="true" />

  <node pkg="tf" type="static_transform_publisher" name="viconworldlink_broadcaster" args="3.98726086e-01   1.31422563e-03  -4.02296235e-06 0.00492303 -0.00330146 -0.00866202  0.99994492 map /viconworld 100" />

  <node pkg="tf" type="static_transform_publisher" name="vicontip_broadcaster" args="0 0 0.1129 0 0 0 /link_6 /vicon_tip 100" /> <!-- for transferring calib tip to link6-->


</launch>
